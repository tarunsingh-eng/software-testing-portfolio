{
	"info": {
		"_postman_id": "5fa649f2-33ea-4010-99f2-1aae9c3c82f1",
		"name": "api-smoke-tests",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "35280522"
	},
	"item": [
		{
			"name": "status",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Query param foo exists\", () => {\r",
							"    const json = pm.response.json();\r",
							"    pm.expect(json.args.foo).to.eql(\"bar\");\r",
							"});\r",
							"\r",
							"pm.test(\"Response has headers\", () => {\r",
							"    const json = pm.response.json();\r",
							"    pm.expect(json.headers).to.be.an(\"object\");\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "https://postman-echo.com/get?foo=bar",
					"protocol": "https",
					"host": [
						"postman-echo",
						"com"
					],
					"path": [
						"get"
					],
					"query": [
						{
							"key": "foo",
							"value": "bar"
						}
					]
				}
			},
			"response": []
		}
	]
}